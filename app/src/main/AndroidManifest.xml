<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="my.project.myprojectfinal.todolist">

    <application
        android:allowBackup="true"
        android:label="@string/nama_aplikasi"
        android:roundIcon="@mipmap/ic_launchers_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="my.project.myprojectfinal.todolist.ui.MemperbaruiActivity" />
        <activity android:name="my.project.myprojectfinal.todolist.ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="my.project.myprojectfinal.todolist.ui.MembuatActivity" />
        <receiver
            android:name="my.project.myprojectfinal.todolist.utilities.Alarm"
            android:enabled="true"
            android:exported="true" />
    </application>

</manifest>